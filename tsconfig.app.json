/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./out-tsc/app",
		"types": ["node"],
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"files": ["src/main.ts", "src/main.server.ts", "server.ts"],
	"include": ["src/**/*.d.ts"]
}
